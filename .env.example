# Dizzy Dish Environment Variables
# Copy this to .env and fill in your values

# API
EXPO_PUBLIC_API_URL=https://api.dizzydish.app/v1

# Instacart Integration
EXPO_PUBLIC_INSTACART_CLIENT_ID=your_instacart_client_id
EXPO_PUBLIC_INSTACART_REDIRECT_URI=dizzydish://instacart/callback

# Social Auth (OAuth client IDs)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id
EXPO_PUBLIC_FACEBOOK_APP_ID=your_facebook_app_id
EXPO_PUBLIC_APPLE_SERVICE_ID=your_apple_service_id

# EAS Project
EXPO_PUBLIC_EAS_PROJECT_ID=your_eas_project_id

# Feature Flags
EXPO_PUBLIC_ENABLE_WEEKLY_PLAN=true
EXPO_PUBLIC_ENABLE_INSTACART=true
