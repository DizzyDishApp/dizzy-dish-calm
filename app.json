{
  "expo": {
    "name": "Dizzy Dish",
    "owner": "dizzydishapp",
    "slug": "dizzydishmain",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "dizzydish",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAF6F1"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.deejkelms.dizzydish",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FAF6F1"
      },
      "package": "com.dizzydish.app"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-asset",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "16e4c09d-9471-47bd-9630-64b50f7c8a3b"
      }
    }
  }
}
